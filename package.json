{
  "name": "strider-slack",
  "version": "0.0.1",
  "description": "Enables strider to fire webhooks to Slack",
  "main": "webapp.js",
  "scripts": {
    "test": "mocha test.js"
  },
  "keywords": [
    "strider",
    "slack",
    "plugin"
  ],
  "author": "Keyvan Fatehi <keyvanfatehi@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "slack-node": "0.0.9"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.20.1",
    "sinon": "^1.10.2",
    "sinon-chai": "^2.5.0"
  },
  "strider":{
    "type": "job",
    "id":"slack",
    "title": "Slack",
    "worker": "worker.js",
    "webapp": "webapp.js",
    "icon": "icon.png",
    "config": {
      "controller": "SlackController"
    }
  }
}
